<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_impress_agirl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.genius.android.howtoimpressagirl.Impress.ImpressAGirl"
    tools:showIn="@layout/app_bar_impress_agirl">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:id="@+id/AyurvedicHerbs"
        android:textSize="10pt"
        android:textColor="#000000"
        android:fontFamily="serif"
        />


    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/listView"
        android:layout_marginTop="55dp"
        android:paddingLeft="20dp"
        android:background="@drawable/backlist"/>



</RelativeLayout>
